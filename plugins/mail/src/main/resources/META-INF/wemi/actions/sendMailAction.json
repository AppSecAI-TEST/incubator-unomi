{
    "id": "sendMailAction",
    "serviceFilter": "(actionExecutorId=sendMail)",
    "tags": [ "event" ],
    "parameters": [
        {
            "id": "from",
            "type": "string",
            "multivalued": false,
            "choiceListInitializerFilter": ""
        },
        {
            "id": "to",
            "type": "string",
            "multivalued": false,
            "choiceListInitializerFilter": ""
        },
        {
            "id": "cc",
            "type": "string",
            "multivalued": false,
            "choiceListInitializerFilter": ""
        },
        {
            "id": "bcc",
            "type": "string",
            "multivalued": false,
            "choiceListInitializerFilter": ""
        },
        {
            "id": "subject",
            "type": "string",
            "multivalued": false,
            "choiceListInitializerFilter": ""
        },
        {
            "id": "template",
            "type": "string",
            "multivalued": false,
            "choiceListInitializerFilter": "",
            "defaultValue": "Hello $profile.properties.(\"j:firstName\")$ $profile.properties.(\"j:lastName\")$,\nHow are you doing today ?"
        }

    ]
}