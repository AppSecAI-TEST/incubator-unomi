{
    "metadata" : {
        "id": "goal1onHover",
        "name": "Sets property goal 1 on hover",
        "description" : "Sets property goal1=reached when event hover is generated with appropriate content name"
    },

    "condition" : {
        "type": "hoverEventCondition",
        "parameterValues": {
            "contentName": "testHover"
        }
    },

    "actions" : [
        {
            "type": "setPropertyAction",
            "parameterValues": {
                "setPropertyName": "goal1",
                "setPropertyValue": "reached"
            }
        }
    ]

}