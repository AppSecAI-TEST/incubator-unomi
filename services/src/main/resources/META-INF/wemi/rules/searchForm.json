{
    "metadata" : {
        "id": "searchForm",
        "name": "Search form submit detection",
        "description" : "Search form submit detection",
        "readOnly":false
    },

    "condition" : {
        "parameterValues": {
            "formId":"searchForm"
        },
        "type": "formEventCondition"
    },

    "actions" : [
        {
            "type": "sendEventAction",
            "parameterValues": {
                "eventType":"search",
                "eventTarget": {
                    "id": "searchForm",
                    "type" : "form",
                    "properties" : {}
                },
                "eventProperties": {
                    "origin": "eventProperty::source.path",
                    "originForm": "eventProperty::target.id",
                    "keyword": "script::event.properties['src_terms[0].term'].toLowerCase()"
                }
            }
        }
    ]

}