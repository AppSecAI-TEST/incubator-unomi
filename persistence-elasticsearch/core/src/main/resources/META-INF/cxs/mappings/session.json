{
  "session": {
    "dynamic_templates": [
      {
        "all": {
          "match": "*",
          "match_mapping_type": "text",
          "mapping": {
            "type": "text",
            "analyzer": "folding"
          }
        }
      }
    ],
    "properties": {
      "duration": {
        "type": "long"
      },
      "itemId": {
        "type": "keyword"
      },
      "itemType": {
        "type": "keyword"
      },
      "lastEventDate": {
        "type": "date",
        "format": "strict_date_optional_time||epoch_millis"
      },
      "profileId": {
        "type": "keyword"
      },
      "profile": {
        "properties": {
          "itemId": {
            "type": "keyword"
          },
          "itemType": {
            "type": "keyword"
          }
        }
      },
      "properties": {
        "properties": {
          "location": {
            "type": "geo_point"
          },
          "pageReferringURL": {
            "type": "keyword"
          },
          "referringURL": {
            "type": "keyword"
          }
        }
      },
      "scope": {
        "type": "keyword"
      },
      "size": {
        "type": "long"
      },
      "timeStamp": {
        "type": "date",
        "format": "strict_date_optional_time||epoch_millis"
      }
    }
  }
}